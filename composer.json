{
    "name": "oro/crm-zendesk",
    "description": "OroCRM Zendesk Integration",
    "homepage": "https://github.com/orocrm/OroCRMZendeskBundle.git",
    "license": "OSL-3.0",
    "keywords": ["OroCRM", "Zendesk", "Integration"],
    "authors": [
        {
            "name": "Oro, Inc",
            "homepage": "http://www.orocrm.com"
        }
    ],
    "autoload": {
        "psr-0": {
            "OroCRM\\Bundle\\ZendeskBundle": "src/"
        }
    },
    "target-dir": "src/OroCRM/Bundle/ZendeskBundle",
    "repositories": [
       {
            "type": "composer",
            "url": "http://packagist.orocrm.com"
        }
    ],
    "require": {
        "php": ">=5.4.9",
        "oro/platform": "1.8.x-dev",
        "oro/crm": "1.8.x-dev"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    }
}
